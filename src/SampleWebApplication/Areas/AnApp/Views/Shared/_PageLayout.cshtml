<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AnApp</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link href="~/lib/sircl-src-new/sircl.css" rel="stylesheet" />
</head>
<body data-bs-theme="light">

    <header>
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">AnApp</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" aria-current="page" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" aria-current="page" asp-action="Page1">Page 1</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" aria-current="page" asp-action="Page2">Page 2</a>
                        </li>
                    </ul>
                    <ul class="navbar-nav float-end">
                        <li class="nav-item">
                            --  @DateTime.Now.ToLongTimeString()
                        </li>
                    </ul>

                </div>
            </div>
        </nav>
    </header>

    <div class="container" sircl-appid="AnApp">
        @RenderBody()
    </div>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="~/lib/sircl-src-new/sircl.js"></script>
    <script src="~/lib/sircl-src-new/sircl-extended.js"></script>
    <script src="~/lib/sircl-src-new/sircl-bootstrap5.js"></script>
</body>
</html>
