<!DOCTYPE html>
<html>
<head>
    <!--#FULL_PAGE#-->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MvcDashboardIdentity by Arebis</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/gh/codetuner/sircl/dist/sircl-1.1.3/sircl-full.min.css" rel="stylesheet" />
    <style>
        #body-content {
            margin-bottom: 32px;
        }

        h2 {
            padding-top: 36px;
            padding-bottom: 24px;
        }

        .pagination .page-item {
            min-width: 48px;
            text-align: right;
        }

            .pagination .page-item input[type=radio] {
                display: none;
            }

        .red-bordered {
            border: solid 2px red;
            padding: 1px;
        }

        .overlay {
            background: rgba(0,0,0,0.33);
            position: absolute;
            padding: 0;
            margin: 0;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            border-radius: .25rem;
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/codetuner/sircl/dist/sircl-1.1.3/sircl-full.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/codetuner/sircl/dist/sircl-1.1.3/sircl-bootstrap4.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/codetuner/sircl/dist/sircl-1.1.3/sircl-fa.min.js"></script>
    @RenderSection("Scripts", required: false)
</head>
<body>

    <div class="container-fluid">

        <div class="navbar-expand-sm">
            <vc:mvc-dashboard-identity-nav-bar />
        </div>

        <div id="body-content">
            @RenderBody()
        </div>

        <div class="modal fade" id="modal-lg" tabindex="-1" role="dialog" onclose-reset=":content">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content" id="modal-lg-content">
                    <div class="modal-body text-center">
                        <div style="font-size: 3em; color: darkgray; margin: 2em;">
                            <i class="fas fa-spinner fa-spin"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</body>
</html>