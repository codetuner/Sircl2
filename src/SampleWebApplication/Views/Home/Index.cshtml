    <div class="tab-pane fade show active" id="forms" role="tabpanel" aria-labelledby="home-tab">

        <div class="card">
            <div class="card-body">
                <p><b>Autocomplete using form:</b></p>
                <form class="onchange-submit" asp-controller="Products" asp-action="AutoComplete" asp-route-listid="autoprod" method="get" target="#autoprod">
                    <input class="oninput-change" type="text" list="autoprod" name="value" autocomplete="off" />
                    <datalist id="autoprod"></datalist>
                </form>
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <p><b>Autocomplete without form:</b></p>
                <form action="/Home/Privacy" method="get">
                    <input class="oninput-change onchange-nosubmit substitute-fields" type="text" name="prod" list="autoprod2" autocomplete="off" onchange-action="@Url.Action("AutoComplete", "Products", new { age="[age]", date = "[date]" })" browser-cache="on" target="#autoprod2" />
                    <datalist id="autoprod2"></datalist>
                    <p>
                        <input type="text" value="45" name="age" />
                    </p>
                </form>
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <p><b>#fr1 File upload - IndexDelayed</b></p>
                <form id="fr1" action="/Home/IndexDelayed" method="post" enctype="multipart/form-data">

                    <p>
                        <input id="f1" name="f1" type="file" required />
                    </p>

                    <p>
                        <input id="x0" name="date" type="text" required />
                    </p>

                </form>
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <p><b>#frm2 Various submit methods</b></p>
                <form id="frm2" default-submit-button="#x4" class="with-overlay" method="get" onsubmit-disable=">:input" onchange-set="IsDirty" xx-protect-message="Leave this form ?">

                    <p>
                        <input name="IsDirty" type="text" class="float-right" readonly />
                        <input id="x1" name="age" type="text" class="oninput-change" value="123" required autofocus />
                    </p>

                    <p>
                        <button id="x2" type="button" onclick="alert('hi');">Say hi</button>
                    </p>

                    <p>
                        <button id="x3" type="submit" formaction="/Customers" onclick-confirm="Sure ?" class="protect-ignore"><i class="spinner"></i>Submit / confirm / Lists customers</button>
                    </p>

                    <p>
                        <button id="x4" formaction="/Home/Privacy" name="inv" value="12" target="#bs-modal-content"><i class="spinner">[]</i> Default-Submit / Privacy in modal</button>
                    </p>

                    <p>
                        <button id="x5" class="btn btn-primary" onload-toggleclass="btn-primary, btn-dark, alert alert-primary on < p">No formaction</button>
                    </p>

                    <p>
                        <button id="x6" type="button" onclick="document.getElementById('frm2').submit();">Submit by script</button>
                    </p>

                </form>

                <p>
                    <button type="button" onclick="$('#frm2').show('fast');">Show</button>
                    <button type="button" onclick="$('#frm2').hide('fast');">Hide</button>
                </p>
            </div>
        </div>
    </div>

