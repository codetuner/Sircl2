@{
    ViewData["Title"] = "Home Page";
}



<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

@if (!ViewBag.IsDataSeeded)
{
    <div class="alert alert-info">
        <h3>Database is empty.</h3>
        <form asp-action="SeedData" method="post">
            <button type="submit" class="btn btn-primary btn-lg">Seed database</button>
        </form>
    </div>
}

<!-- Dialogs -->

<style>
    dialog {
        z-index: 700;
    }
</style>

<dialog class="dialog-exclusive">
    <div>
        <h5>Dialog 1</h5>
        <div id="dlg1">
        </div>
        <div>
            <button class="btn btn-primary onclose-restore onclick-closedialog">Close</button>
        </div>
    </div>
</dialog>

<dialog class="dialog-modal dialog-exclusive">
    <div>
        <h5>Dialog 2</h5>
        <div id="dlg2">
        </div>
        <div>
            <button class="btn btn-primary onclick-closedialog">Close</button>
        </div>
    </div>
</dialog>


<dialog class="onload-showdialog dialog-modal dialog-exclusive">
    <div>
        <h5>Dialog 3</h5>
        <div id="dlg3">
        </div>
        <div>
            <button class="btn btn-primary onclick-closedialog">Close</button>
        </div>
    </div>
</dialog>

<dialog class="onload-showdialog onclose-restore dialog-modal dialog-exclusive dialog-nocancel" id="rd889">
    <div>
        <h5>RED DIALOG</h5>
        <div id="reddialog" @*download-progress="progress"*@>
            <p>Please wait...</p>
            @*<progress></progress>*@
        </div>
        <div>
            <button class="btn btn-primary onclick-closedialog">Close</button>
        </div>
    </div>
</dialog>

<dialog class="" open-after="00:10">
    <div>
        <h5>Welcome Dialog</h5>
        <div>
            <p>Welcome !</p>
        </div>
        <div>
            <button class="btn btn-primary onclose-restore onclick-closedialog">Close</button>
        </div>
    </div>
</dialog>

<!-- Modals -->

<div class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Modal 1</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="bsmodal1">
                <p>Modal body text goes here.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>

